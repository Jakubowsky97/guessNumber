<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane minHeight="400.0" minWidth="300.0" prefHeight="400.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.gornik.guessnumber.Controller">
   <children>
      <Button fx:id="ExitBtn" layoutX="220.0" layoutY="350.0" mnemonicParsing="false" prefHeight="36.0" prefWidth="66.0" text="Wyjście" />
      <Label layoutX="69.0" layoutY="20.0" text="Zgadnij liczbę">
         <font>
            <Font name="System Bold" size="25.0" />
         </font>
      </Label>
      <TextField fx:id="numberArea" layoutX="21.0" layoutY="81.0" onAction="#checkNumber" prefHeight="25.0" prefWidth="260.0" AnchorPane.leftAnchor="21.0" AnchorPane.rightAnchor="19.0" />
      <Label fx:id="infoLabel" alignment="CENTER" layoutX="84.0" layoutY="120.0" text="Wpisz liczbę od 1 do 100." textFill="RED" />
      <Button fx:id="reloadBtn" layoutX="12.0" layoutY="350.0" mnemonicParsing="false" prefHeight="36.0" prefWidth="124.0" text="Uruchom ponownie" />
      <Label fx:id="numberOfTries" layoutX="75.0" layoutY="144.0" prefHeight="17.0" prefWidth="154.0" />
   </children>
</AnchorPane>
